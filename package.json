{
    "name": "konbini",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "workspaces": {
        "packages": [
            "packages/*"
        ],
        "catalog": {
            "@zakahacecosas/number-utils": "npm:@jsr/zakahacecosas__number-utils@latest",
            "@zakahacecosas/string-utils": "npm:@jsr/zakahacecosas__string-utils@latest",
            "yaml": "^2.8.0",
            "typescript": "~5.8.3",
            "@types/bun": "latest",
            "@supabase/supabase-js": "^2.50.2"
        }
    },
    "scripts": {
        "pretty": "prettier --w .",
        "lint": "eslint . --cache --fix && tsc --noEmit",
        "run:cli": "bun packages/client/src/index.ts",
        "run:upd": "bun packages/update/src/index.ts",
        "run:web": "cd packages/gui && bun run dev",
        "build": "echo 'Building the CLI...' && bun run bld:cli && echo 'Building the GUI...' && bun run bld:web",
        "bld:cli": "bun packages/client/build.ts",
        "bld:web": "cd packages/gui && bun run build",
        "launch:web": "cd packages/gui && bun run prod",
        "launch:web:bld": "bun run bld:web && cd packages/gui && bun run prod",
        "guard": "bun packages/client/guard/avs.ts"
    },
    "dependencies": {
        "eslint": "^9.30.0",
        "prettier": "^3.6.2"
    },
    "trustedDependencies": [
        "@vercel/speed-insights"
    ]
}
